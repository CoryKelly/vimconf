{
  "languageserver": {
    "graphql": {
    "command": "graphql-lsp",
    "args": ["server", "-m", "stream"],
    "filetypes": ["typescript", "typescriptreact", "graphql"]
  },
    "golang": {
      "command": "gopls",
      "rootPatterns": ["go.mod", ".vim/", ".git/", ".hg/"],
      "filetypes": ["go"]
    }
  }
}

